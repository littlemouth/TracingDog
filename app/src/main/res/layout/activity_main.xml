<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.MainActivity">
    <RelativeLayout
        android:id="@id/main_activity_title_relativelayout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/common_bg_green"
        android:gravity="center_vertical">
        <TextView
            android:id="@id/main_title_label_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textColor="@android:color/white"
            android:layout_marginLeft="20dp"/>
    </RelativeLayout>
    <dfst.com.ui.widget.TabView
        android:id="@id/main_activity_tabview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/main_activity_title_relativelayout"></dfst.com.ui.widget.TabView>

</RelativeLayout>
